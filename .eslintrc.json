{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "settings": {
        "import/resolver": "webpack"
    },
    "plugins": [
        "react"
    ],
    "ignorePatterns": ["src/core/public/**", "node_modules/**", "webpack.config.js"],
    "rules": {
        "comma-dangle": 0
    }
}
